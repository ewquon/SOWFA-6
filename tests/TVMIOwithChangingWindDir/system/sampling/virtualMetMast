    refMet
    {
        type                probes;
        functionObjectLibs ("libsampling.so");
        name                refMet;
        writeControl        timeStep;
        writeInterval       1;
        fields
        (
            U
        );

        probeLocations
        (
            (0.001 0.001  10.0)
            (0.001 0.001  30.0)
            (0.001 0.001  50.0)
            (0.001 0.001  70.0)
            (0.001 0.001  90.0)
